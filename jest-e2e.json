{
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": ".",
    "testEnvironment": "node",
    "collectCoverage": true,
    "verbose": true,
    "testMatch": ["<rootDir>/test/**/*.e2e-spec.{ts,js}"],
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage-e2e"
}